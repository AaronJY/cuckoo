{
	"name": "Twitter Metric Hider",
	"description": "Hides twitter like, reply and retweet counts.",
	"version": "0.1",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*"],
			"js": ["hider.js", "content.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": ["storage"]
}